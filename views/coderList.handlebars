<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/coderList">Home</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a class="nav-link" href="/company/:companyName">Profile<span class="sr-only">(current)</span></a>
            </li>
        </ul>
    </div>
</nav>


<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Heyyy Here are Some Genius</h1>
    <p class="lead">This is a modified jumbotron that occupies the entire horizontal space of its parent.</p>
  </div>
</div>


{{#each coderList}}
<div class="position-static">
    <div class="coderListCard">
        <img style="width: 300px" height="300px" src={{this.coderPic}}/>
        <p class="coderListName">Full Name: {{this.name}} {{this.lastname}}</p>
        <p class="coderLevel">Level: {{this.level}}</p>
        <p class="coderContr">Number of Contributions: {{this.numberContr}}</p>
    </div> 
    {{#each skills}}
        <div>
            <p class="coderSkills">{{this.skills}}</p>
        </div>
    {{/each}}


</div>
{{/each}}