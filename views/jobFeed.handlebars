<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/jobFeed">Job Feed</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>
    <ul class="navbar-nav">
        <li class="nav-item active">
            <a class="nav-link" href="/user/:username">Developers<span class="sr-only">(current)</span></a>
        </li>
    </ul>
    </div>
</nav>

<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4">Heyyy Here are Some New Projects For you</h1>
        <p class="lead">This is a modified jumbotron that occupies the entire horizontal space of its parent.</p>
    </div>
</div>

{{#each jobPost}}
<div class="position-static">

    <hr>
    <div class="cartFeed">
        <div id="companyLogo"><img style="width: 300px" height="300px" src={{this.logo}} />
            <h3>{{this.companyName}}</h3>
        </div>
        <div class="cartDescription">
            <p id="jobDetails">{{this.jobDescription}}</p>
            <ul id="techList">
                {{#each this.techList}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
            <p>Add this job to your list</p>
            <button id="btnAddJob" class="btn-primary" type="submit">Add</button>
        </div>
    </div>

</div>
{{/each}}